msgstr "<20>{#p/undyne}{#e/undyne/9}...誰是老大！"

msgid "<20>{#p/undyne}{#e/undyne/7}... RIGHT NOW!"
msgstr "<20>{#p/undyne}{#e/undyne/7}...幹掉你！ゑ"

