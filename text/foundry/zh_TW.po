msgstr "<20>我怎能不讓你吃點苦頭！"

msgid ""
"<20>{#e/undyne/36}Alphys...\n"
"Asgore..."
msgstr ""
"<20>{#e/undyne/36}Alphys...\n"
"Asgore..."

msgid ""
"<20>{#e/undyne/36}Alphys...\n"
"Asgore...\n"
"Papyrus..."
msgstr ""
"<20>{#e/undyne/36}Alphys...\n"
"Asgore...\n"
"Papyrus..."

