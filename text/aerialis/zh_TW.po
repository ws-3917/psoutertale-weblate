msgstr ""
"<25>{#p/asriel2}{#f/3}* ...\n"
"* $(name)？ゑ{%200}"

msgid "<25>{#p/asriel2}{#f/13}* $(name).{%200}"
msgstr "<25>{#p/asriel2}{#f/13}* $(name)。ゑ{%200}"

msgid ""
"<25>{#p/asriel2}{#f/7}* We could've been off the outpost by this "
