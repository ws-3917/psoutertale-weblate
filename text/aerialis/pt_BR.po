msgstr "<20>{#p/mettaton}SEM OFENSA, MAS VOCÊ NÃO É MUITO BOM NISSO."

msgid ""
"<20>{#p/mettaton}...\n"
"MAYBE YOU SHOULD TRY DOING SOMETHING ELSE."
msgstr ""
"<20>{#p/mettaton}...\n"
"TALVEZ VOCÊ DEVESSE TENTAR FAZER ALGUMA OUTRA COISA."

