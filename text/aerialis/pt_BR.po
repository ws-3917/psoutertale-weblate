msgstr "<18>{*}... estamos saindo do ar."

msgid "<18>{#p/mettaton}{*}... absolutely fantastic!"
msgstr "<18>{#p/mettaton}{*}... absolutamente fantásticos!"

msgid "<18>{#p/mettaton}{*}... absolutely beautiful!"
msgstr "<18>{#p/mettaton}{*}... absolutamente lindos!"

