msgstr "<32>{#p/alphys}* 别吧。"

msgid ""
"<08>{#p/basic}{~}Hey!\n"
"We don't do that here."
msgstr ""
"<08>{#p/basic}{~}嘿！\n"
"我们这\n"
"没这说法。"

msgid ""
"<08>{#p/basic}{~}Well..\n"
"If it's what you're into.."
msgstr ""
"<08>{#p/basic}{~}好吧..\n"
"如果你\n"
"为此\n"
"感兴趣.."

