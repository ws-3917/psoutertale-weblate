msgstr "<25>{#p/asriel2}{#f/13}* 别磨蹭...ゑ{%200}"

msgid "<25>{#p/asriel2}{#f/1}* There."
msgstr "<25>{#p/asriel2}{#f/1}* 搞定。ゑ"

