msgid ""
"<25>{#f/13}* Even...\n"
"* Canis Major's d-death..."
msgstr ""
"<25>{#f/13}* 甚至...\n"
"* Canis Major的死-死亡..."

msgid ""
"<25>{#f/13}* Even...\n"
"* Canis Minor's d-death..."
msgstr ""
"<25>{#f/13}* 甚至...\n"
"* Canis Minor的死-死亡..."

