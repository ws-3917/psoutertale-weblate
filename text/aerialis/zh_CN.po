msgstr "<32>{#p/human}* （你的钱不够。）"

msgid "<25>{#p/sans}{#f/0}* you don't even have 5G?"
msgstr "<25>{#p/sans}{#f/0}* 你连5G都没有？"

msgid ""
"<25>{#p/sans}{#f/3}* ... here.\n"
"* take some of mine."
msgstr ""
"<25>{#p/sans}{#f/3}* 要不...\n"
"* 我给你点钱吧。"

