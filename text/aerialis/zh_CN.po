msgid "<32>{#p/alphys}* No..."
msgstr "<32>{#p/alphys}* 不..."

msgid "<32>{#p/story}* Final Froggit stands its ground."
msgstr "<32>{#p/story}* Final Froggit绝不退缩。"

msgid "* Flutterknyte"
msgstr "* Flutterknyte"

