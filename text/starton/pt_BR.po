msgid ""
"<18>{#f/9}ANYWAY!!!\n"
"THAT'S ENOUGH TALKING!!!"
msgstr ""
"<18>{#f/9}POIS BEM!!!\n"
"JÁ CHEGA DE CONVERSA!!!"

