msgstr "<18>VOLTA AQUI!!!"

msgid "<18>{#p/papyrus}{#f/4}BACK AGAIN, EH?"
msgstr "<18>{#p/papyrus}{#f/4}MAIS UMA VEZ?"

msgid "<18>{#f/5}I SUPPOSE IT'S MY FAULT..."
msgstr "<18>{#f/5}EU SUPONHO QUE SEJA MINHA CULPA..."

