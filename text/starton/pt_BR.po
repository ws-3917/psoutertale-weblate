msgstr "<18>UM ALÍVIO TE VER POR AQUI..."

msgid ""
"<18>{#f/7}...\n"
"WAIT A SECOND!!!"
msgstr ""
"<18>{#f/7}...\n"
"ESPERA UM POUCO!!!"

