msgstr ""
"<11>{#p/basic}{~}（喘气 \n"
"喘气）"

msgid "<11>{#p/basic}{~}(Tiny bark)"
msgstr ""
"<11>{#p/basic}{~}（轻微的\n"
"犬叫声）"

