msgstr ""
"<11>{#p/basic}{~}我的天呐，\n"
"抚摸永不停歇！！"

msgid "<11>{#p/basic}{~}AHHHHHHH!!!"
msgstr "<11>{#p/basic}{~}汪呜——！！！"

msgid "<11>{#p/basic}{~}No escape!"
msgstr "<11>{#p/basic}{~}别想逃！"

