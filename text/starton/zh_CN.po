msgstr "<11>{#p/basic}{~}居然还在摸！"

msgid "<11>{#p/basic}{~}AND COMING!!"
msgstr "<11>{#p/basic}{~}还在摸！！"

msgid "<11>{#p/basic}{~}AND COMINGGG!!!"
msgstr ""
"<11>{#p/basic}{~}还\n"
"在\n"
"摸！！！"

